                        <span class="button">&nbsp;&nbsp; Close &nbsp;&nbsp;</span>
                    </a>
                </div>
            </div>

            <!-- space -->
            <div class="manager-bottom-space"></div>
            <!-- space -->
                </div>
                <!-- content-area-end -->
            </div>
            
        </div>
