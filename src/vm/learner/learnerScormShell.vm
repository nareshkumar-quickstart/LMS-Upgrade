					$thisModal.addClass('pre-loader please-wait');
						setTimeout(function(){
							var bodyHtml =	'<h1>Support</h1>'+
											'<p>Find 24-hour support for any online course here. Access our knowledge base, submit a support ticket, troubleshoot issues, recover passwords, chat, e-mail or call our customer support desk, and more. We look forward to helping you.</p>'+
											'<div>'+
												'<a href="https://www.quickstart.com/contact-us/" target="_blank" class="cd-btn main-action">Get Support</a>'+												
											'</div>';
							$thisModal.removeClass('pre-loader please-wait').html(bodyHtml);
						},100);
				break;
						
				case "idle":
					var $thisModal = $('.cd-modal[data-modal="'+ attr +'"] > .cd-modal-content');
						$thisModal.addClass('pre-loader please-wait');
							setTimeout(function(){
								var bodyHtml =	'<h1>Your session is about to expire.</h1>'+
												'<p>If you are still working in your course simply click the "Stay & Continue" button.'+
												'<br>Your session will expire in <strong class="primary-font">4:56 minutes.</strong></p>'+
												'<div>'+
													'<a href="javascript:ui.svgModal.close(\'modal-dynamic\');" type="button" class="cd-btn main-action">Stay & Continue</a>'+
												'</div>';
								$thisModal.removeClass('pre-loader please-wait').html(bodyHtml);
							},100);
				break;
				case "recommendation":
					$(".cd-modals").addClass("cd-modal-with-nav");
					var $thisModal = $('.cd-modal[data-modal="'+ attr +'"] > .cd-modal-content');
						$thisModal.addClass('pre-loader please-wait');
							setTimeout(function(){
								var bodyHtml =	'<h1>Related Courses</h1>'+
												'<p>Based on the course, you may be interested in some of the following:</p>'+
												'<div id="isoTope-enabled">'+
													'<div class="iso-container">'+
														'<div id="iso-filter-btns">'+
														  '<button type="button" class="active" data-filter="*">All</button>'+
														  '<button type="button" data-filter=".courses">Courses</button>'+
														  '<button type="button" data-filter=".packages">Packages</button>'+
														  '<button type="button" data-filter=".subs">Subscriptions</button>'+
														'</div>'+
														'<div class="btn-group sortBy">'+
														  '<button type="button" title="Sorting By" class="btn" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"></button>'+
														  '<ul class="dropdown-menu dropdown-menu-right">'+
															'<li><a href="#">Sort Ascending (A-Z)</a></li>'+
															'<li><a href="#">Sort Descending (Z-A)</a></li>'+
														  '</ul>'+
														'</div>'+
													'</div>'+
													'<div id="course_recomendations" class="iso-main"></div>'+
													'<div class="load-more">'+
														'<a href="javascript:;" onclick="ui.isoTope.getData(false);" class="cd-btn main-action">See More Courses</a>'+
													'</div>'+
