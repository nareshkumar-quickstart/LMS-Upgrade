				break;
				case "discussions":
					$(".cd-modals").addClass("cd-modal-with-nav");
					var $thisModal = $('.cd-modal[data-modal="'+ attr +'"] > .cd-modal-content');
					$thisModal.addClass('pre-loader please-wait');
					ui.discussions.createSpace($thisModal);
				break;
			 
				case "clipp":
					$(".cd-modals").addClass("cd-modal-with-nav");
					var $thisModal = $('.cd-modal[data-modal="'+ attr +'"] > .cd-modal-content');
					$thisModal.addClass('pre-loader please-wait');
					ui.clipp.load($thisModal);
