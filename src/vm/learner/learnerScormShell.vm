			$(".for-assessment").addClass("hide");
			
			var templateName = dummyOutline[slideAt];
			ui.slide.dummyLoad(templateName,function(){
			
				//	initialize this template base transition
				ui.slide.transition(dummyOutline[slideAt]);
				
				//	hide main course player loader
				ui.loader("hide", function()
				{
					setTimeout(function()
					{
						$("#wrapper").addClass("toggled-top toggled-bottom toggled-left");
						/*setTimeout(function()
						{
							ui.nav.move("outline",'600px');
						},3000);*/
						
					},1000);
				},"launching");
			});
		}
		
		function course_exit()
		{
			//	this should be call when all information saved
			ui.loader("show", function()
			{
				window.open("exit.html","_self");
			},"saving");
		}
		
		function svg_modal_open($trg)
		{
			var attr = $trg.attr("data-group");
			var trg = $trg.attr("data-trg");
			var heading = $trg.attr("data-term");
			$(".cd-modals").removeClass("cd-modal-with-nav");
			switch(trg)
			{
				case 'save':
					var $thisModal = $('.cd-modal[data-modal="'+ attr +'"] > .cd-modal-content');
					var bodyHtml;
					if(true)
					{
						bodyHtml =	'<h1>Your Assessment Is Not Complete</h1>'+
										'<p>If you logout, your assessment will be graded. All unanswered questions will be marked as incorrect.</p>'+
										'<div>'+
											'<a href="javascript:course_exit();" type="button" class="cd-btn main-action">Exit Course</a>'+
