        function doEnter(event){
            if(event==null){
                event = window.event;
            }
            var unicode=event.which? event.which : event.keyCode;
            if (unicode == 13){
                if(document.all){//ie
                    window.event.cancelBubble = true;
                    window.event.returnValue = false;
                    search();
                }else{//firefox
                    if(event.cancelable){
                        event.preventDefault();
                    }
                    search();
                }
            }
        }

